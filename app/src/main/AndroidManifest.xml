<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.example.administrator.courcemanager">

    <application
            android:allowBackup="true"
            android:icon="@mipmap/ic_launcher"
            android:label="掌上课堂"
            android:roundIcon="@mipmap/ic_launcher_round"
            android:supportsRtl="true"
            android:theme="@style/AppTheme">
        <activity android:name=".activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".activity.SecondActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.DEFAULT"/>
            </intent-filter>
        </activity>
        <activity android:name=".student.StudentActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.DEFAULT"/>
            </intent-filter>
        </activity>
        <activity android:name=".teacher.TeacherActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.DEFAULT"/>
            </intent-filter>
        </activity>
        <activity android:name=".student.StudentEditActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.DEFAULT"/>
            </intent-filter>
        </activity>
        <activity android:name=".teacher.TeacherEditActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.DEFAULT"/>
            </intent-filter>
        </activity>
        <activity android:name=".manager.ManagerActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.DEFAULT"/>
            </intent-filter>
        </activity>
        <activity android:name=".manager.TeacherManageActivity">
        </activity>
        <activity android:name=".manager.StudentManageActivity">
        </activity>
        <activity android:name=".student.StudentAttendActivity">
        </activity>
        <activity android:name=".teacher.TeacherAttendActivity">
        </activity>
        <activity android:name=".student.StudentScoreActivity">
        </activity>
        <activity android:name=".teacher.TeacherScoreActivity">
        </activity>
        <activity android:name=".student.StudentSevaluaActivity">
        </activity>
        <activity android:name=".teacher.TeacherSevaluaActivity">
        </activity>
    </application>

</manifest>